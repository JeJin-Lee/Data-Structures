#include <stdio.h>

void BubbleSort(int arr[], int n)
{
    int i, j, temp;
    
    for(i=0; i<n-1; i++){
        for(j=i+1; j<(n-i)-1; j++){
            if(arr[j]>arr[j+1]){
                temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }
}
